<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
   <Appenders>
     <Console name="console" target="SYSTEM_OUT">
       <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} [%-5p](%-35c{1}:%-3L) - %m%n" />
     </Console>
   </Appenders>
   <Loggers>
     <Root level="INFO">
       <AppenderRef ref="console" />
     </Root>
  </Loggers>
</Configuration>
